@client.message_handler(commands = ['get_info', 'info'])
def get_user_info(message):
	markup_inline = types.InlineKeyboardMarkup()
	item_yes = types.InlineKeyboardButton(text = '–î–ê', callback_data = 'yes')
	item_no = types.InlineKeyboardButton(text = '–ù–ï–¢', callback_data = 'no')

	markup_inline.add(item_yes, item_no)
	client.send_message(message.chat.id, '–ñ–µ–ª–∞–µ—Ç–µ —É–∑–Ω–∞—Ç—å –Ω–µ–±–æ–ª—å—à—É—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –≤–∞—Å?',
		reply_markup = markup_inline
	)

@client.callback_query_handler(func = lambda call: True)	
def on_answer(call):
	if call.data == 'yes':
		markup_reply = types.ReplyKeyboardMarkup(resize_keyboard = True)
		item_id = types.KeyboardButton('–ú–û–ô ID')
		item_username = types.KeyboardButton('–ú–û–ô –ù–ò–ö')

		markup_reply.add(item_id, item_username)
		client.send_message(call.message.chat.id, '–ù–∞–∂–º–∏—Ç–µ –Ω–∞ –æ–¥–Ω—É –∏–∑ –∫–Ω–æ–ø–æ–∫',
				reply_markup = markup_reply
		)
	elif call.data == 'no':
		pass

@client.message_handler(content_types=["text"])
def send_echo(message):
    mgr = owm.weather_manager()
    observation = mgr.weather_at_place('Ukraine')
    w = observation.weather

    if message.text == '–ú–û–ô ID':
	    client.send_message(message.chat.id, f'Your ID: {message.from_user.id}')
    elif message.text == '–ú–û–ô –ù–ò–ö':
	    client.send_message(message.chat.id, f'Your NICNAME: {message.from_user.first_name} {message.from_user.last_name}')	
    elif message.text.lower() == '–ø—Ä–∏–≤–µ—Ç':
	    client.send_message(message.chat.id, '–ü—Ä–∏–≤–µ—Ç–∏–∫–∏üòò')
    elif message.text.lower() == '–∫–∞–∫ –¥–µ–ª–∞?':
	    client.send_message(message.chat.id, '–¢–∞ –≤—Å–µ –ø—É—Ç–µ–º, –∫–∞–∫ —Å–∞–º?üôÉ')
    elif message.text.lower() == '–∫—Ç–æ —Ç–µ–±—è —Å–æ–∑–¥–∞–ª?':
	    client.send_message(message.chat.id, '–†—É–∫–æ–∂–æ–øüò´')
    elif message.text.lower() == 'c':
        client.send_message(message.chat.id, str(random.randint(0,300)))
    elif message.text.lower() == '–∫–∞–∫ —Ç–µ–±—è –∑–æ–≤—É—Ç?':
	    client.send_message(message.chat.id, '–¥–ª—è —Ç–µ–±—è –º–æ–∂–Ω–æ –ø—Ä–æ—Å—Ç–æ —Ä–∞–±üòé')
    elif message.text.lower() == '—Å–∫–æ–ª—å–∫–æ —Ç–µ–±–µ –ª–µ—Ç?':
	    client.send_message(message.chat.id, '–ú–µ–Ω—å—à–µ —á–µ–º —Ç–µ–±–µ')
    elif message.text.lower() == '–ø–æ–∫–∞':
	    client.send_message(message.chat.id, '–ü–æ–∫–∞–∫–∞–µ—à –¥–æ–º–∞!...–ö–æ–≥–¥–∞ –Ω–∞—Å—Ç—É–ø–∏–ª —Ç–æ—Ç –ø–µ—Ä–µ–ª–æ–º–Ω—ã–π –º–æ–º–µ–Ω—Ç –∫–æ–≥–¥–∞ —è –Ω–∞—á–∞–ª –≤–µ—Å—Ç–∏ —Å–µ–±—è –∫–∞–∫ –¥–æ–ª–±–æ–µ–±...–ê! –û–Ω –∂–µ –∏ –Ω–µ –Ω–∞—Å—Ç—É–ø–∞–ª!...üòü')
