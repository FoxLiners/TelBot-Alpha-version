@client.message_handler(commands = ['get_info', 'info'])
def get_user_info(message):
	markup_inline = types.InlineKeyboardMarkup()
	item_yes = types.InlineKeyboardButton(text = 'ДА', callback_data = 'yes')
	item_no = types.InlineKeyboardButton(text = 'НЕТ', callback_data = 'no')

	markup_inline.add(item_yes, item_no)
	client.send_message(message.chat.id, 'Желаете узнать небольшую информацию о вас?',
		reply_markup = markup_inline
	)

@client.callback_query_handler(func = lambda call: True)	
def on_answer(call):
	if call.data == 'yes':
		markup_reply = types.ReplyKeyboardMarkup(resize_keyboard = True)
		item_id = types.KeyboardButton('МОЙ ID')
		item_username = types.KeyboardButton('МОЙ НИК')

		markup_reply.add(item_id, item_username)
		client.send_message(call.message.chat.id, 'Нажмите на одну из кнопок',
				reply_markup = markup_reply
		)
	elif call.data == 'no':
		pass

@client.message_handler(content_types=["text"])
def send_echo(message):
    mgr = owm.weather_manager()
    observation = mgr.weather_at_place('Ukraine')
    w = observation.weather

    if message.text == 'МОЙ ID':
	    client.send_message(message.chat.id, f'Your ID: {message.from_user.id}')
    elif message.text == 'МОЙ НИК':
	    client.send_message(message.chat.id, f'Your NICNAME: {message.from_user.first_name} {message.from_user.last_name}')	
    elif message.text.lower() == 'привет':
	    client.send_message(message.chat.id, 'Приветики😘')
    elif message.text.lower() == 'как дела?':
	    client.send_message(message.chat.id, 'Та все путем, как сам?🙃')
    elif message.text.lower() == 'кто тебя создал?':
	    client.send_message(message.chat.id, 'Рукожоп😫')
    elif message.text.lower() == 'c':
        client.send_message(message.chat.id, str(random.randint(0,300)))
    elif message.text.lower() == 'как тебя зовут?':
	    client.send_message(message.chat.id, 'для тебя можно просто раб😎')
    elif message.text.lower() == 'сколько тебе лет?':
	    client.send_message(message.chat.id, 'Меньше чем тебе')
    elif message.text.lower() == 'пока':
	    client.send_message(message.chat.id, 'Покакаеш дома!...Когда наступил тот переломный момент когда я начал вести себя как долбоеб...А! Он же и не наступал!...😟')
    elif message.text.lower() == 'help':
	    client.send_message(message.chat.id, 'Мои фукции:\n и заранее знай вводить надо букву которая стоит в начале кроме j вместо нее пиши /info \n Запомнил? надеюсь ну погнали...\nДля начала нашего общения введи /start\na) Посоветовать игру\nb) Погода (Украина)\nc) Рандомное число\nd) Рандомное эмодзи\ne) Ссылки\nf) Общение \ng) Секрет!\nh) Цитаты\ni) Посоветовать обои на раб. стол\nj)Чтобы узнать свой IP введи /info\nk) визитка')
